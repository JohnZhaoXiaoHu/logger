{
  "name": "@iinfinity/logger",
  "version": "0.3.1",
  "description": "TypeScript / JavaScript colorful logger, support saving logs to file(s).",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "example": "src/demo",
    "test": "src/test"
  },
  "scripts": {
    "build": "tsc",
    "debug": "node -r ts-node/register --inspect-brk src/main/index.ts",
    "demo": "ts-node src/demo",
    "test": "node node_modules/jasmine/bin/jasmine.js --config=jasmine.json --reporter=jasmine-console-reporter --require=ts-node/register"
  },
  "keywords": [
    "logger",
    "log",
    "console"
  ],
  "author": "IInfinity <I.INF@Outlook.com> (https://don.red/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DevinDon/logger/issues"
  },
  "homepage": "https://github.com/DevinDon/logger",
  "private": true,
  "dependencies": {
    "typescript": "latest"
  },
  "devDependencies": {
    "@types/jasmine": "^3.3.12",
    "@types/node": "latest",
    "jasmine": "^3.3.1",
    "jasmine-console-reporter": "^3.1.0",
    "ts-node": "latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevinDon/logger.git"
  }
}
